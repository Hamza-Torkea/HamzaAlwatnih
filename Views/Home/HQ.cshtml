@using alwatnia.Helper;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="">
    <div class="container">
        <div class="new-bar">
            <img class="img-responsive" src="/assets/home/images/parcode.png" alt="" />
        </div>
        <ol style="display: none" class="breadcrumb">
            <li class="breadcrumb-item"><a href="@Url.Action("Index")">@Resource.Main</a></li>
			<li class="breadcrumb-item active">@Resource.HQ</li>
        </ol>
        <h2 style="color: #1e3687" class="title-page">@Resource.HQ</h2>
    </div>
</section><!--section-topBanar-->
<div class="section-contentInner">
    <div class="section-aboutCompany">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="about-block wow fadeInLeft" data-wow-duration="1s" data-wow-delay="0.4s">
                        <img src="@Url.Content("/Recources/Uploads/"+ViewBag.photo+"?w=1200")" alt="" class="img-responsive">
                    </div>
                </div>
            </div>
           
        </div>
    </div><!--section-aboutCompany-->
</div>
